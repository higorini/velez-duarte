{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
    "plugin:react/recommended",
    "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-key": [1, { "checkFragmentShorthand": true }],
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off"
      }
}
